                    <option>IT</option>
                    <option>ETC</option>
                    <option>EI</option>
                    <option>Mech</option>
                    <option>Civil</option>
                </select>
                <label for="typem">Type: </label>
                <select class="form-control" id="typem" name="type" onchange="$('#type').val($(this).children('option:selected').html())">
                    <option value="any">Any</option>
                    <option>Notes</option>
                    <option>Question Papers</option>
                    <option>Books</option>
                    <option>Other</option>
                </select><br>
                <input type="file" class="form-control" name="filetoupload"><br>
                <input type="hidden" id="sem" name="actsem" value="I Year">
                <input type="hidden" id="branch" name="actbr" value="CS">
                <input type="hidden" id="type" name="acttype" value="Notes">
                <button type="submit" class="btn btn-primary btn-md btn-block study-btn" >Submit</button>
              </form>
    </div>
</body>


